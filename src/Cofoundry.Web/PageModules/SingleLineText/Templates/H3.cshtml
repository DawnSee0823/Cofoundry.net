@inherits CofoundryPageModule<SingleLineTextDisplayModel>
@using Cofoundry.Web
@using System.Web.Mvc
@using System.Web.Mvc.Html

@{
    Cofoundry.Module.UseDescription("Wraps the text in an H3 heading tag");
}
<h3>
    @if (Model == null)
    {
        @:<!-- No text data --->
    }
    else
    {
        @:@Model.Text
    }
</h3>