{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },

  "Cofoundry": {
    "Database:ConnectionString": "Server=.\\sqlexpress;Database=Cofoundry.Samples.UserAreas;Integrated Security=True;MultipleActiveResultSets=True",
    "Identity": {
      "Password": {
        "MinLength": 12,
        "MaxLength": 64,
        "MinUniqueCharacters": 7
      },
      "Username": {
        "AllowAnyLetters": true,
        "AllowAnyDigit": true,
        "AdditionalAllowedCharacters": "-._'"
      },
      "EmailAddress": {
        "AllowAnyLetters": true,
        "AllowAnyDigit": true,
        "AdditionalAllowedCharacters": "!#$%&'*+-/=?^_`{|}~.\"(),:;<>@[\\] "
      }
    }
  }
}
