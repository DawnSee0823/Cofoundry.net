<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <configSections xdt:Locator="XPath(/configuration/configSections[last()])">
    <section name="uberCMS" xdt:Locator="Match(name)" xdt:Transform="Remove" />
  </configSections>

  <uberCMS xdt:Transform="Remove">
    <news enabled="false"/>
    <video enabled="false"/>
    <jobs enabled="false"/>
    <pages>
      <compression removeWhitespace="true"/>
    </pages>
  </uberCMS>
</configuration>
